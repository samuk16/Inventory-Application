<%- include('../partials/head') %>

    <body>
        <header>
            <%- include('../partials/nav') %>
        </header>

        <div class="container-authors">
            <% if (locals.tags) { %>
                <% tags.forEach(tag=> { %>
                    <a href="/manhwa?tag=<%= tag.id %>">
                        <div class="author-card">
                            <h2>
                                <%= tag.name_tag %>
                            </h2>
                        </div>
                    </a>
                    <% }) %>
                        <% } %>
        </div>
    </body>

    <%- include('../partials/footer') %>