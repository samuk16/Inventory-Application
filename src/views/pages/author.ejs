<%- include('../partials/head') %>

    <body>
        <header>
            <%- include('../partials/nav') %>
        </header>

        <div class="container-authors">
            <% if (locals.authors) { %>
                <% authors.forEach(author=> { %>
                    <a href="/manhwa?author=<%= author.id %>">
                        <div class="author-card">
                            <h2>
                                <%= author.name_author %>
                            </h2>
                        </div>
                    </a>
                    <% }) %>
                        <% } %>
        </div>
    </body>

    <%- include('../partials/footer') %>